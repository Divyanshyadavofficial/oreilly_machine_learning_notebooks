    "import numpy as np\n",
    "def softmax(Z):\n",
    "    exp_Z = np.exp(Z-np.max(Z,axis=1,keepdims=True))\n",
    "    return exp_Z/np.sum(exp_Z,axis=1,keepdims=True)"
